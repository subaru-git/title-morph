{
  "manifest_version": 3,
  "name": "Title Morph",
  "description": "Change the title based on the URL",
  "version": "1.0",
  "permissions": ["tabs", "activeTab", "storage"],
  "content_scripts": [
    {
      "js": ["change-title.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "setting.html"
  },
  "icons": {
    "16": "icons/icon-16x16.png",
    "32": "icons/icon-32x32.png",
    "192": "icons/icon-192x192.png"
  }
}
